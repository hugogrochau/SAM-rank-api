{
  "name": "rocketleaguesam-api",
  "version": "0.0.3",
  "description": "Rocket League South America API",
  "main": "dist",
  "scripts": {
    "watch": ". ./.env; nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist --ignore node_modules",
    "start": ". ./.env; node dist",
    "prestart": "npm run build",
    "test": ". ./.env.test; mocha --opts ./mocha.opts",
    "test:watch": ". ./.env.test; mocha --watch --opts ./mocha.opts",
    "docs": "apidoc -i src/api -o docs/",
    "migrate": "knex migrate:latest --env development",
    "lint": "eslint src test",
    "lint:staged": "lint-staged",
    "all": "npm run lint && npm run test",
    "updater": ". ./.env; npm run build && node dist/scripts/update.js"
  },
  "lint-staged": {
    "*.js": "lint"
  },
  "pre-commit": "lint:staged",
  "apidoc": {
    "name": "rocketleaguesam-api",
    "version": "0.0.3",
    "description": "Rocket League South America API",
    "apidoc": {
      "description": "Rocket League South America API",
      "url": "https://127.0.0.1:8080/api/v1/"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugogrochau/rocketleaguesam-api.git"
  },
  "author": "Hugo Grochau <hugo@grochau.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "body-parser": "^1.16.0",
    "bookshelf": "^0.10.3",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "express-mung": "^0.4.6",
    "express-validator": "^3.1.2",
    "isomorphic-fetch": "^2.2.1",
    "jwt-simple": "^0.5.1",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "morgan": "^1.8.0",
    "openid": "^2.0.6",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^6.1.2",
    "resource-router-middleware": "^0.6.0",
    "rocketleaguesam-api-client": "^0.3.3",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "jsend": "^1.0.2",
    "lint-staged": "^3.3.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2"
  },
  "keywords": [
    "south",
    "america",
    "rocket",
    "league",
    "rank",
    "leaderboard"
  ]
}
