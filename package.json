{
  "name": "rocketleaguesam-api",
  "version": "0.0.2",
  "description": "Rocket League South America API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist --ignore node_modules",
    "start": "node -r dotenv/config dist",
    "prestart": "npm run -s build",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --reporter spec",
    "docs": "apidoc -i src/api -o apidoc/",
    "migrate": "knex migrate:latest --env development",
    "lint": "eslint src test",
    "all": "npm run lint && npm run test",
    "updater": "npm run -s build && node -r dotenv/config dist/scripts/update.js"
  },
  "apidoc": {
    "name": "sam-rank-api",
    "version": "0.0.2",
    "description": "Rocket League South America API",
    "apidoc": {
      "description": "Rocket League South America API",
      "url": "https://127.0.0.1:8080/api/v1/"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hugogrochau/rocketleaguesam-api.git"
  },
  "author": "Hugo Grochau <hugo@grochau.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "body-parser": "^1.16.0",
    "bookshelf": "^0.10.3",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-validator": "^3.1.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonapi-mapper": "^1.0.0-beta.10",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "pg": "^6.1.2",
    "resource-router-middleware": "^0.6.0",
    "validator": "^6.2.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.4",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sqlite3": "^3.1.8"
  },
  "keywords": [
    "south",
    "america",
    "rocket",
    "league",
    "rank",
    "leaderboard"
  ]
}
